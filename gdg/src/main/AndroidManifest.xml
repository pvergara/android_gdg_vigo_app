					android:label="@string/app_name"
					android:theme="@style/AppTheme">


		<activity android:name="ameiga.saulmm.gdg.gui.activities.MainActivity">
			<intent-filter>
				<action android:name="android.intent.action.MAIN"/>
				<category android:name="android.intent.category.LAUNCHER"/>
			</intent-filter>
		</activity>

		<activity android:name="ameiga.saulmm.gdg.gui.activities.Test">


		</activity>

	</application>

</manifest>
