<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    android:versionCode="1"
    android:versionName="AndroidGDGVigoApp"
    package="ameiga.saulmm.gdg" >

    <uses-sdk
        android:minSdkVersion="10"
        android:targetSdkVersion="19" />

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/gdg_icon"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity android:name="ameiga.saulmm.gdg.gui.activities.MainActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="ameiga.saulmm.gdg.gui.activities.Test" >
        </activity>
    </application>

</manifest>